{
    "name": "diego/grepper",
    "description": "A simple tool to grep specific lines of a textfile.",
    "require": {
        "symfony/console": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "diego",
            "email": "diego@sirdiego.de"
        }
    ],
    "minimum-stability": "stable",
    "bin": ["bin/grepper"],
    "autoload": {
        "psr-4": {
            "Diego\\Grepper\\": "src/Diego/Grepper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diego\\Grepper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mikey179/vfsStream": "^1.6"
    }
}
